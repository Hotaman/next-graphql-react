{
  "name": "next-graphql-react",
  "version": "3.0.0",
  "description": "Next.js config and App decorators for graphql-react, enabling server side rendered GraphQL operations.",
  "license": "MIT",
  "author": {
    "name": "Jayden Seric",
    "email": "me@jaydenseric.com",
    "url": "https://jaydenseric.com"
  },
  "repository": "github:jaydenseric/next-graphql-react",
  "homepage": "https://github.com/jaydenseric/next-graphql-react#readme",
  "bugs": "https://github.com/jaydenseric/next-graphql-react/issues",
  "keywords": [
    "next",
    "graphql",
    "react",
    "esm",
    "mjs"
  ],
  "files": [
    "universal",
    "server"
  ],
  "main": "universal",
  "engines": {
    "node": ">=8.5"
  },
  "peerDependencies": {
    "graphql-react": "^8.0.0",
    "next": "7 - 8",
    "react": "^16.8.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.3",
    "object-assign": "^4.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-replace-object-assign": "^2.0.0",
    "eslint": "^5.16.0",
    "eslint-config-env": "^4.0.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.17.1",
    "eslint-plugin-import-order-alphabetical": "^0.0.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-hooks": "^1.6.0",
    "graphql-react": "^8.2.0",
    "husky": "^1.3.1",
    "jsdoc-md": "^2.1.0",
    "lint-staged": "^8.1.5",
    "next": "^8.0.4",
    "prettier": "^1.17.0",
    "react": "^16.8.6"
  },
  "scripts": {
    "prepare": "npm run prepare:clean && npm run prepare:mjs && npm run prepare:js && npm run prepare:jsdoc && npm run prepare:prettier",
    "prepare:clean": "rm -rf universal server",
    "prepare:mjs": "BABEL_ESM=1 babel src -d . --keep-file-extension",
    "prepare:js": "babel src -d .",
    "prepare:jsdoc": "jsdoc-md",
    "prepare:prettier": "prettier '{universal,server}/**/*.{mjs,js}' readme.md --write",
    "test": "npm run test:eslint && npm run test:prettier",
    "test:eslint": "eslint . --ext mjs,js",
    "test:prettier": "prettier '**/*.{json,yml,md}' -l",
    "prepublishOnly": "npm test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{mjs,js}": "eslint",
    "*.{json,yml,md}": "prettier -l"
  },
  "eslintIgnore": [
    "!.*",
    "node_modules",
    "/universal",
    "/server"
  ],
  "eslintConfig": {
    "extends": [
      "env"
    ]
  },
  "prettier": {
    "proseWrap": "never",
    "singleQuote": true,
    "semi": false
  }
}
